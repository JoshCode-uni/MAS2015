module getArmour {

	program {
		%% Goal Handling %%
			% If I want and see armor, get armor.
			if bel( wantArmor(Armor), item(Location,_, Armor,_) ) then adopt( armor(Location) ).
		%% Execution %%
			%if your goal is to the armor at an location, go to the armor.
			if goal( armor(Location) ) then navigate(Location).
	}

}